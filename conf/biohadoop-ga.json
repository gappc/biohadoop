{
  "version" : "0.1",
  "includePaths" : [ "/biohadoop/lib/", "/biohadoop/conf/" ],
  "applicationConfigs" : [ {
    "name" : "GA-LOCAL-1",
    "algorithmConfiguration" : {
      "@algorithmConfigClass" : "at.ac.uibk.dps.biohadoop.ga.config.GaAlgorithmConfig",
      "dataFile" : "/biohadoop/data/att48.tsp",
      "populationSize" : 10,
      "maxIterations" : 100
    },
    "algorithm" : "at.ac.uibk.dps.biohadoop.ga.algorithm.Ga",
    "persistenceConfiguration" : {
      "@persistenceConfigurationClass" : "at.ac.uibk.dps.biohadoop.persistencemanager.file.FilePersistenceConfiguration",
      "saveConfiguration" : {
        "@persistenceSaveConfigurationClass" : "at.ac.uibk.dps.biohadoop.persistencemanager.file.FileSaveConfiguration",
        "path" : "/biohadoop/persistence/ga",
        "afterIterations" : 1000
      },
      "loadConfiguration" : {
        "@persistenceLoadConfigurationClass" : "at.ac.uibk.dps.biohadoop.persistencemanager.file.FileLoadConfiguration",
        "path" : "/biohadoop/persistence/ga",
        "onStartup" : true
      }
    }
  } ],
  "connectionConfiguration" : {
    "masters" : [ {
      "endpoints" : [ "at.ac.uibk.dps.biohadoop.ga.master.socket.GaSocket", "at.ac.uibk.dps.biohadoop.ga.master.kryo.GaKryo", "at.ac.uibk.dps.biohadoop.ga.master.rest.GaRest", "at.ac.uibk.dps.biohadoop.ga.master.websocket.GaWebSocket" ]
    } ],
    "workers" : {
      "at.ac.uibk.dps.biohadoop.ga.worker.SocketGaWorker" : 3,
      "at.ac.uibk.dps.biohadoop.ga.worker.WebSocketGaWorker" : 1,
      "at.ac.uibk.dps.biohadoop.ga.worker.RestGaWorker" : 1,
      "at.ac.uibk.dps.biohadoop.ga.worker.KryoGaWorker" : 1
    }
  },
  "distributionConfiguration" : {
    "host" : "master",
    "port" : 2181
  }
}